{
  "description": "Polymer 3 element that provides a table of contents for use in a document library or other such use.",
  "keywords": [
    "element-collection",
    "web-components",
    "polymer",
    "menu"
  ],
  "name": "@xgd/xgd-table-of-contents",
  "version": "3.1.0",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "main": "xgd-table-of-contents.js",
  "contributors": [
    "XGDFalcon LLC",
    "xgdfalcon@gmail.com"
  ],
  "dependencies": {
    "@polymer/iron-flex-layout": "^3.0.0-pre.21",
    "@polymer/iron-icon": "^3.0.0-pre.21",
    "@polymer/iron-icons": "^3.0.0-pre.21",
    "@polymer/paper-input": "^3.0.0-pre.21",
    "@polymer/paper-item": "^3.0.0-pre.21",
    "@polymer/paper-toggle-button": "^3.0.0-pre.21",
    "@polymer/paper-tooltip": "^3.0.0-pre.21",
    "@polymer/polymer": "^3.0.0"
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.0.0-pre.21",
    "auto-changelog": "^1.7.2",
    "wct-browser-legacy": "^1.0.1"
  },
  "auto-changelog": {
    "issueUrl": "https://github.com/XGDElements/xgd-table-of-contents/issues/{id}",
    "issuePattern": "[A-Z]+-\\d+",
    "replaceText": {
      "(ABC-\\d+)": "[`$1`](https://github.com/XGDElements/xgd-table-of-contents/issues/$1)"
    }
  },
  "bugs": {
    "url": "https://github.com/XGDElements/xgd-table-of-contents/issues/"
  },
  "homepage": "https://github.com/XGDElements/xgd-table-of-contents",
  "repository": {
    "type": "git",
    "url": "https://github.com/XGDElements/xgd-table-of-contents.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "reinstall": "rm -rf node_modules polymer-lock.json && npm install",
    "preversion": "auto-changelog --template keepachangelog -p",
    "version": "git add CHANGELOG.md && git commit -a --amend --no-edit",
    "postversion": "git push --force && git push --tags --force"
  }
}
